{
  "name": "decision-assistant",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "node --watch --enable-source-maps dist/server.js",
    "verify:guardrail": "npm run build && npx tsx scripts/verify_guardrail.ts",
    "verify:ai_momentum_override": "npm run build && node scripts/verify_ai_momentum_override.js",
    "release:local": "npm run verify:guardrail",
    "test": "vitest",
    "test:semantics": "vitest tests/receipt_semantics.test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
